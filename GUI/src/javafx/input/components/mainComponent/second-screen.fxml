<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="500.0" maxWidth="0.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0"
            prefWidth="830.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="javafx.input.components.mainComponent.SecondScreenController">
    <top>
        <Label styleClass="header-label" text="Predictions" BorderPane.alignment="CENTER">
            <font>
                <Font size="25.0"/>
            </font>
        </Label>
    </top>
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <GridPane hgap="5.0" vgap="5.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="NEVER" maxWidth="30.0" minWidth="10.0" prefWidth="10.0"/>
                        <ColumnConstraints hgrow="NEVER" maxWidth="85.0" prefWidth="85.0"/>
                        <ColumnConstraints hgrow="NEVER" maxWidth="352.0" minWidth="23.0" prefWidth="49.0"/>
                        <ColumnConstraints hgrow="ALWAYS" maxWidth="900.0" minWidth="10.0" prefWidth="478.0"/>
                        <ColumnConstraints hgrow="NEVER"/>
                        <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="572.0" spacing="10.0"
                              GridPane.columnIndex="1">
                            <children>
                                <Button fx:id="openFileButton" maxHeight="40.0" maxWidth="85.0" minHeight="40.0"
                                        minWidth="85.0" mnemonicParsing="false" onAction="#openFileButtonActionListener"
                                        prefHeight="40.0" prefWidth="80.0" text="Load File ">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                        <Button fx:id="simulationsQueueButton" contentDisplay="RIGHT" maxHeight="40.0" maxWidth="192.0"
                                minHeight="40.0" minWidth="165.0" mnemonicParsing="false"
                                onAction="#simulationsQueueButtonActionListener" prefHeight="40.0" prefWidth="192.0"
                                text="Queue Management" GridPane.columnIndex="4">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </Button>
                        <Label id="text" styleClass="file-label" text="File: " GridPane.columnIndex="2">
                            <padding>
                                <Insets left="10.0"/>
                            </padding>
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </Label>
                        <Label fx:id="selectedFileName" GridPane.columnIndex="3"/>
                    </children>
                </GridPane>
                <Separator prefHeight="10.0" prefWidth="830.0">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets bottom="10.0"/>
                    </VBox.margin>
                </Separator>
                <GridPane hgap="5.0" prefHeight="315.0" prefWidth="830.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="410.0" minWidth="10.0" prefWidth="315.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="566.0" minWidth="10.0" prefWidth="510.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <ScrollPane hbarPolicy="NEVER" prefHeight="315.0" prefWidth="0.0">
                            <content>
                                <FlowPane fx:id="entitiesFlowPane" hgap="5.0" prefHeight="312.0" prefWidth="313.0"
                                          vgap="5.0"/>
                            </content>
                        </ScrollPane>
                        <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
                            <content>
                                <FlowPane fx:id="envVarsFlowPane" hgap="5.0" prefHeight="313.0" prefWidth="507.0"
                                          vgap="5.0"/>
                            </content>
                        </ScrollPane>
                    </children>
                </GridPane>
                <Separator prefHeight="10.0" prefWidth="830.0">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </Separator>
                <GridPane hgap="5.0" vgap="5.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="185.0" minWidth="10.0" prefWidth="86.0"/>
                        <ColumnConstraints hgrow="ALWAYS" maxWidth="950.0" minWidth="10.0" prefWidth="601.0"/>
                        <ColumnConstraints halignment="RIGHT" hgrow="NEVER" maxWidth="319.0" minWidth="-Infinity"
                                           prefWidth="83.0"/>
                        <ColumnConstraints halignment="RIGHT" hgrow="NEVER" maxWidth="10.0" minWidth="10.0"
                                           prefWidth="10.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Button fx:id="clearButton" maxHeight="40.0" maxWidth="85.0" minHeight="40.0" minWidth="85.0"
                                mnemonicParsing="false" onAction="#clearButtonActionListener" prefHeight="40.0"
                                prefWidth="80.0" text="Clear" GridPane.columnIndex="1"/>
                        <Button fx:id="startButton" maxHeight="40.0" maxWidth="85.0" minHeight="40.0" minWidth="85.0"
                                mnemonicParsing="false" onAction="#startButtonActionListener" prefHeight="40.0"
                                prefWidth="80.0" text="Start" GridPane.columnIndex="3"/>
                    </children>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </GridPane>
            </children>
        </VBox>
    </center>
</BorderPane>
