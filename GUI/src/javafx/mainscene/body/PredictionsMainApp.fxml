<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="100.0" minWidth="100.0"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="javafx.mainscene.body.PredictionsMainAppController">
    <content>
        <BorderPane>
            <center>
                <TabPane fx:id="tabPane" maxHeight="5000.0" maxWidth="5000.0" tabClosingPolicy="UNAVAILABLE"/>
            </center>
            <top>
                <HBox alignment="TOP_CENTER" BorderPane.alignment="CENTER">
                    <children>
                        <fx:include fx:id="headerComponent" source="../header/Header.fxml"/>
                    </children>
                </HBox>
            </top>
        </BorderPane>
    </content>
</ScrollPane>
