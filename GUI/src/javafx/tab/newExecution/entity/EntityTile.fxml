<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="180.0" maxWidth="260.0" minHeight="63.0" minWidth="154.0" prefHeight="66.0"
      prefWidth="250.0" styleClass="round-layout" stylesheets="@Entity.css" xmlns="http://javafx.com/javafx/20.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.tab.newExecution.entity.EntityController">
    <children>
        <Label fx:id="entityNameLabel" alignment="CENTER" contentDisplay="CENTER" maxHeight="50.0"
               maxWidth="1.7976931348623157E308" minHeight="28.0" prefHeight="36.0" prefWidth="248.0"/>
        <TextField fx:id="entityPopulationTextField" maxHeight="26.0" maxWidth="80.0" minHeight="20.0" minWidth="80.0"
                   onAction="#setPopulationActionListener" prefHeight="26.0" prefWidth="80.0"/>
    </children>
    <padding>
        <Insets bottom="5.0"/>
    </padding>
</VBox>
