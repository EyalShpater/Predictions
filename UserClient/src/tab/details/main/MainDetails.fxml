           prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="tab.details.main.MainDetailsController">
    <children>
        <SplitPane dividerPositions="0.1" orientation="VERTICAL">
            <items>
                <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
