                    <children>
                        <HBox alignment="CENTER_LEFT">
                            <children>
                                <ChoiceBox fx:id="worldChoiceBox" onMouseClicked="#onMouseClicked" prefWidth="150.0"/>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </HBox>
                    </children>
                </StackPane>
                <fx:include source="../details/Details.fxml"/>
            </items>
        </SplitPane>
    </children>
</StackPane>
